<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<system.webServer>
		<security>
			<requestFiltering removeServerHeader="true" />
		</security>
		<httpProtocol>
			<customHeaders>
				<!-- Remove Other Headers -->
				<remove name="X-Powered-By" />

			</customHeaders>
		</httpProtocol>
	</system.webServer>
</configuration>