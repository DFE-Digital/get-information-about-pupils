{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft": "Warning",
            "Microsoft.Hosting.Lifetime": "Information"
        }
    },
    "AllowedHosts": "*",
    "ContainerOptions": {
        "PupilContainerName": "pupil-noskill",
        "PupilPremiumContainerName": "pupil-premium-v2",
        "FurtherEducationContainerName": "further-education-v2",
        "ApplicationDataContainerName": "application-data",
        "ReferenceDataContainerName": "reference"
    },
    "RepositoryOptions": {
        "EndpointUri": "",
        "PrimaryKey": "",
        "DatabaseId": "giapsearch",
        "ConnectionMode": 1,
        "Containers": [
            {
                "application-data": {
                    "ContainerName": "application-data",
                    "PartitionKey": "/DOCTYPE"
                }
            },
            {
                "further-education": {
                    "ContainerName": "further-education-v2",
                    "PartitionKey": "/ULN"
                }
            },
            {
                "pupil-noskill": {
                    "ContainerName": "pupil-noskill",
                    "PartitionKey": "/PupilMatchingRef"
                }
            },
            {
                "pupil-premium": {
                    "ContainerName": "pupil-premium-v2",
                    "PartitionKey": "/PupilMatchingRef"
                }
            },
            {
                "reference": {
                    "ContainerName": "reference",
                    "PartitionKey": "/DOCTYPE"
                }
            },
            {
                "news": {
                    "ContainerName": "news",
                    "PartitionKey": "/id"
                }
            },
            {
                "users": {
                    "ContainerName": "users",
                    "PartitionKey": "/id"
                }
            }
        ]
    },
    "SearchIndexOptions": {
        "Url": "AZURESEARCH_URL",
        "Key": "AZURESEARCH_KEY",
        "Indexes": {
            "npd": {
                "Name": "NPD-INDEX-NAME"
            },
            "pupil-premium": {
                "Name": "PUPILPREMIUM-INDEX-NAME"
            }
        }
    },
    "SecurityHeaders": {
        "Remove": [
            "Server",
            "X-Powered-By"
        ],
        "Add": {
            "Strict-Transport-Security": "max-age=31536000; includeSubDomains; preload",
            "X-XSS-Protection": "0",
            "X-Content-Type-Options": "nosniff",
            "X-Frame-Options": "DENY",
            "Content-Security-Policy": "default-src 'self'; script-src 'self' https://*.clarity.ms https://www.googletagmanager.com 'nonce-{nonce}'; connect-src 'self' https://*.clarity.ms;"
        }
    }
}
